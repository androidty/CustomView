<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ty.customview.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.DisplayViewActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AnimationNumActivity" />
        <activity android:name=".activity.LoadingWaveActivity" />
        <activity android:name=".activity.PendulumActivity" />
        <activity android:name=".activity.BowActivity" />
        <activity android:name=".activity.BezierActivity" />
        <activity android:name=".activity.RingProgressActivity" />

        <service android:name=".util.killSelfService" />
    </application>

</manifest>